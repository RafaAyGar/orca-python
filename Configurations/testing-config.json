{


	"general_conf": {

		"basedir": "Datasets/",
		"datasets": ["tae", "balance-scale", "contact-lenses"],
		"folds": 3,
		"jobs": 10,
		"runs_folder": "my_runs/",
		"metrics": ["ccr", "mae", "mze"],
		"cv_metric": "mae"
	},


	"configurations": {

		"SVMOP": {
		
			"classifier": "OrdinalDecomposition",
			"parameters": {
				"dtype": "OrderedPartitions",
				"decision_method": "frank_hall",
				"classifier": "sklearn.svm.SVC",
				"parameters": {
					"C": [0.1, 1, 10],
					"gamma": [0.1, 1, 10],
					"probability": ["True"]
				}

			}
		}




	}





}
